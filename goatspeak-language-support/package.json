{
  "name": "goat-language-support",
  "displayName": "goat-language-support",
  "description": "Language support for the Goatspeak language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "goat",
      "aliases": ["Goatspeak", "goat"],
      "extensions": [".goat",".g",".goatspeak"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "goat",
      "scopeName": "source.goat",
      "path": "./syntaxes/goat.tmLanguage.json"
    }]
  }
}
